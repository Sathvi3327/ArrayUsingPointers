#include<string>
using namespace std;
int main()
{
	int i;                      //Declaring variables
	int arr[i];
	cout<<"Enter elements: ";   //Taking in input
	for(i=0;i<5;i++)
	{
		cin>>arr[i];
	}
	int *p;                     //Declaring pointer
	p = arr;
	cout<<"You Entered: ";      //Giving out output
	for(int i=0;i<5;i++)
	{
		cout<<*p<<endl;
		p++;
	}
	return 0;	
}
